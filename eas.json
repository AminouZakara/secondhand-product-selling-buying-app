{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "ENVFILE": ".env"
      }
    },
    "production": {
      "developmentClient": false,
      "distribution": "store",
      "env": {
        "ENVFILE": ".env.production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
